{
  "name": "vite-plugin-lazy-routes",
  "version": "0.0.12",
  "type": "module",
  "description": "Use Remix lazy flat-route routing in your Vite project",
  "license": "MIT",
  "scripts": {
    "build": "node scripts/build",
    "build:watch": "npm run build --watch",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "fast-glob": "^3.2.12",
    "minimatch": "^7.4.3",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@types/node": "^18.15.5",
    "@types/react": "^18.0.28",
    "react": "^18.2.0",
    "react-router-dom": "^6.9.0",
    "tsup": "^6.7.0",
    "vite": "^4.2.1"
  },
  "peerDependencies": {
    "react": ">=16.6",
    "react-router-dom": "^6.9.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/argarcia-ottersoft/vite-plugin-lazy-routes"
  },
  "homepage": "https://github.com/argarcia-ottersoft/vite-plugin-lazy-routes",
  "bugs": "https://github.com/argarcia-ottersoft/vite-plugin-lazy-routes/issues",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./virtual": {
      "types": "./virtual.d.ts"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "client": [
        "dist/client"
      ]
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "virtual.d.ts"
  ],
  "keywords": [
    "vite",
    "react",
    "remix",
    "run",
    "routes",
    "routing",
    "router",
    "pages",
    "plugin",
    "file",
    "based",
    "lazy"
  ],
  "engines": {
    "node": ">=16"
  },
  "volta": {
    "node": "16.18.0"
  }
}
